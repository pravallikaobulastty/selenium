package basicprograms;
import java.util.Scanner;
public class Newnumber {

	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		int sum=0,a,b,num,a1,b1,sum1=0;
		num=scan.nextInt();
		a=num%10;
		b=num/10;
		a=a+1;
		sum=sum*10+a;
		a1=sum%10;
		b1=sum/10;
		sum1=a1*10+b1;
		a=b%10;
		b=b/10;
		a=a+1;
		sum=sum*10+a;
		a1=sum%10;
		b1=sum/10;
		sum1=a1*10+b1;
		a=b%10;
		b=b/10;
		a=a+1;
		sum=sum*10+a;
		a1=sum%10;
		b1=sum/10;
		sum1=a1*10+b1;
		a=b%10;
		b=b/10;
		a=a+1;
		sum=sum*10+a;
		a1=sum%10;
		b1=sum/10;
		sum1=a1*10+b1;
		a=b%10;
		a=a+1;
		sum=sum*10+a;
		a1=sum%10;
		b1=sum/10;
		sum1=a1*10+b1;
		System.out.println("New number="+sum1);
	}

}
